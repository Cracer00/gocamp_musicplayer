  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.11",
    "@grpc/proto-loader": "^0.7.5",
    "jest": "^29.4.3",
    "mongodb": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.4.3"
  }
}
