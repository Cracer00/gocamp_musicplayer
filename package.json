{
  "name": "golangcampnode",
  "version": "1.0.0",
  "description": "A playlist module for GoLang camp 2023",
  "main": "playlist.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "protoc": {
    "options": [
      "--proto_path=proto"
